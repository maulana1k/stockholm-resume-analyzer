{
  "name": "stockholm-resume-analyzer",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "build:exe": "bun build src/index.ts --compile --outfile dist/main",
    "start": "bun run dist/index.js",
    "db:push": "bunx prisma db push",
    "db:seed": "bun scripts/seed-vector-db.ts",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:up-db": "docker compose up -d postgres redis qdrant",
    "docker:down": "docker compose down",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "@types/pdf-parse": "^1.1.4",
    "prisma": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "1.1.3",
    "@prisma/client": "^5.0.0",
    "@qdrant/qdrant-js": "^1.15.1",
    "bullmq": "^5.58.8",
    "dotenv": "^17.2.2",
    "hono": "^4.0.0",
    "ioredis": "^5.8.0",
    "openai": "^4.0.0",
    "pino": "^9.11.0",
    "pino-pretty": "^13.1.1",
    "zod": "^3.0.0"
  }
}
